{
  "compilerOptions": {
    "target": "esnext",
    "module": "commonjs",
    "moduleResolution": "node",
    "allowJs": false,
    "noEmit": true,
    "strict": true,
    "isolatedModules": true,
    "noImplicitAny": false,
    "esModuleInterop": true,
    "lib": [
      "esnext"
    ]
  },
  "exclude": [
    "node_modules"
  ]
}